  "login-content#modal__login--mobile" : {

  },
  "icon#user-section__login--icon" : {
    "title" : "icon user section login",
    "props" : {
      "id" : "hpa-profile",
      "width" : 24
    }
  },
  "icon#user-section__searchbar--icon" : {
    "title" : "icon user section searchbar",
    "props" : {
      "id" : "hpa-search",
      "width" : 24
    }
  }
